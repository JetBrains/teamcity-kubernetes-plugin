<?xml version="1.0" encoding="UTF-8"?>


<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:schemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>teamcity-kubernetes-plugin</name>
    <display-name>Kubernetes support</display-name>
    <version>@Version@</version>
    <description>Allows running TeamCity agents in a Kubernetes cluster</description>
    <vendor>
      <name>JetBrains</name>
      <url>http://www.jetbrains.com</url>
    </vendor>
  </info>
  <deployment use-separate-classloader="true" node-responsibilities-aware="true"/>
</teamcity-plugin>